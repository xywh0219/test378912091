<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>
   
	<package name="sheet" namespace="/sheet" extends="sys">
    <action name="sheetAction" class="sheetAction">
      <result name="success">/sheet/admSheetManage.jsp</result>
      <result name="fail">/sheet/sfail.jsp</result>
      <result name="play">/sheet/memSheetPlay.jsp</result>      
      <result name="playsheet">/audio/aplay.jsp</result>
    </action>
	</package>
	 
 
</struts>